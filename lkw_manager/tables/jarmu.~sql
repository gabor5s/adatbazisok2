create table jarmu(
       id                        number not null primary key,
       jarmu_tipus               varchar2(50) not null,
       rendszam                  varchar2(6),
       vasarlasi_datum           date,
       forgalomba_helyezes       date,
       evjarat                   number,
       futott_km                 number,
       teherbiras                number,
       teljesitmeny              int,
       sofor_id                  number not null,
       telep_id                  number not null,
       last_mod                  timestamp(6),
       created                   timestamp(6),
       mod_user                  varchar2(300),
       dml_flag	                 varchar2(1),
       version	                 number
)
tablespace users;

